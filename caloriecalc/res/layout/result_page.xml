<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"

	android:orientation="vertical">
	<TextView android:id="@+id/results_page_txt_summary"
		android:textSize="30dip" android:layout_width="wrap_content"
		android:layout_marginTop="30dip" android:layout_marginBottom="30dip"
		android:layout_height="wrap_content" android:text="@string/result_page_txt_summary"></TextView>


	<TextView android:layout_height="wrap_content"
		android:id="@+id/results_page_txt_kcal_quemadas" android:layout_width="wrap_content"
		android:text="Kcal" android:layout_gravity="center"
		android:layout_marginTop="30dip" android:textSize="30dip"
		android:layout_weight="1"></TextView>
</LinearLayout>
